<!DOCTYPE kpartgui>
<kpartgui name="kverbos" version="6">
<MenuBar>
  <Menu name="file"><text>&amp;File</text>
    <Action name="file_open_standard" append="new_merge"/>
    <Action name="file_new_window"/>
  </Menu>
  <Menu name="edit"><text>&amp;Edit</text>
    <Action name="edit_erfassen"/>
    <Action name="edit_bearbeiten"/>
  </Menu>
  <Menu name="settings"><text>&amp;Settings</text>
    <Action name="settings_options" append="configure_merge"/>
  </Menu>
  <Menu name="user"><text>&amp;User</text>
    <Action name="user_username"/>
    <Action name="user_results"/>
  </Menu>
</MenuBar>
<ToolBar name="mainToolBar" noMerge="1"><text>Main Toolbar</text>
  <Action name="file_new" />
  <Action name="file_open" />
  <Action name="file_save" />
  <Action name="file_print" />
  <Separator/> 
  <Action name="edit_cut" />
  <Action name="edit_copy" />
  <Action name="edit_paste" />
</ToolBar>

</kpartgui>

