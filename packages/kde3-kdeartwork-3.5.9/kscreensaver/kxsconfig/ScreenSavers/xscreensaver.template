[Desktop Entry]
Encoding=UTF-8
Exec=@NAME@
Icon=kscreensaver
Type=Application
Actions=InWindow,Root,Setup
Name=@LABEL@
TryExec=xscreensaver
@GL1@
@GL2@

[Desktop Action Setup]
Exec=kxsconfig @NAME@
Name=Setup...

[Desktop Action InWindow]
Exec=kxsrun @NAME@ -- -window-id %w
Name=Display in Specified Window
NoDisplay=true

[Desktop Action Root]
Exec=kxsrun @NAME@ -- -root
Name=Display in Root Window
NoDisplay=true

