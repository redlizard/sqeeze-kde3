                        <cstring>textLabel1</cstring>
                    </property>
                    <property name="text">
                        <string>Reconnection attempts:</string>
                    </property>
                </widget>
                <widget class="KIntNumInput" row="0" column="1">
                    <property name="name">
                        <cstring>kcfg_ReconnectDelay</cstring>
                    </property>
                    <property name="label">
                        <string></string>
                    </property>
                    <property name="value">
                        <number>120</number>
                    </property>
                    <property name="minValue">
                        <number>1</number>
                    </property>
                    <property name="maxValue">
                        <number>9999</number>
                    </property>
                    <property name="suffix">
                        <string> seconds</string>
                    </property>
                </widget>
                <widget class="KIntNumInput" row="1" column="1">
                    <property name="name">
                        <cstring>kcfg_ReconnectCount</cstring>
                    </property>
                    <property name="value">
                        <number>100</number>
                    </property>
                    <property name="minValue">
                        <number>0</number>
                    </property>
