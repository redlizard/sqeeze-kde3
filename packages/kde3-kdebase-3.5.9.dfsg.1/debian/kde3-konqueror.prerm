#! /bin/sh -e

case "$1" in
    remove)
        /usr/sbin/update-alternatives --remove x-www-browser /usr/bin/konqueror
    ;;

    upgrade|deconfigure)
    ;;

    failed-upgrade)
    ;;

    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0
